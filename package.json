{
  "name": "simulator",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/app.js",
    "check:lint": "eslint . --ext .ts,.js",
    "check:pretty": "prettier --check \"*/**/*.{ts,json}\"",
    "check": "npm run check:lint && npm run check:pretty",
    "fix:lint": "eslint . --ext .ts,.js --fix",
    "fix:pretty": "prettier --write \"*/**/*.{ts,json}\"",
    "fix": "npm run fix:lint && npm run fix:pretty",
    "prepare": "husky"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true
  },
  "dependencies": {
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "typescript": "^5.5.2"
  }
}
